/// 字典类型表
model SysDictType {
  dictId     BigInt    @id @default(autoincrement()) @map("dict_id") /// 字典主键
  dictName   String    @default("") @map("dict_name") @db.VarChar(100) /// 字典名称
  dictType   String    @unique @map("dict_type") @db.VarChar(100) /// 字典类型
  status     String    @default("0") @map("status") @db.Char(1) /// 状态（0正常 1停用）
  createBy   String?   @default("") @map("create_by") @db.VarChar(64) /// 创建者
  createTime DateTime? @default(now()) @map("create_time") @db.DateTime(0) /// 创建时间，可空
  updateBy   String?   @default("") @map("update_by") @db.VarChar(64) /// 更新者
  updateTime DateTime? @updatedAt @map("update_time") @db.DateTime(0) /// 更新时间
  remark     String?   @map("remark") @db.VarChar(500) /// 备注

  dictData SysDictData[]

  @@map("sys_dict_type")
}
