/// 用户岗位关联表
model SysUserPost {
  user_id BigInt @map("user_id") /// 用户id
  post_id BigInt @map("post_id") /// 岗位ID

  user SysUser @relation(fields: [user_id], references: [user_id], onDelete: Cascade)
  post SysPost @relation(fields: [post_id], references: [post_id], onDelete: Cascade)

  @@id([user_id, post_id])
  @@map("sys_user_post")
}
