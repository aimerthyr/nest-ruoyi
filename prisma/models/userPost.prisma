/// 用户岗位关联表
model SysUserPost {
  userId BigInt @map("user_id") /// 用户id
  postId BigInt @map("post_id") /// 岗位ID

  user SysUser @relation(fields: [userId], references: [userId], onDelete: Cascade)
  post SysPost @relation(fields: [postId], references: [postId], onDelete: Cascade)

  @@id([userId, postId])
  @@map("sys_user_post")
}
